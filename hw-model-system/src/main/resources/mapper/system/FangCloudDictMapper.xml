<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.suixingpay.hw.system.mapper.FangCloudDictMapper">

    <resultMap id="result" type="com.suixingpay.hw.system.domain.FangCloudDict">
        <id column="id" property="id"/>
        <result column="domain_name" property="domainName"/>
        <result column="domain_desc" property="domainDesc"/>
        <result column="value" property="value"/>
        <result column="value_desc" property="valueDesc"/>
    </resultMap>

    <select id="selectDictDataByDomainName" parameterType="java.lang.String" resultMap="result">
        select id, domain_name, domain_desc, value, value_desc
        from t_domains_values
        where domain_name = #{domainName}
    </select>
</mapper>